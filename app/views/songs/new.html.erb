<h1> list a new song </h1>

<%= form_for @song do |f| %>
    <%= f.label :name, "Name: " %>
    <%= f.text_field :name %>
    <%= f.label :artist_name, "Artist: " %>
    <%= f.text_field :artist_name %>
    <%= f.label :genre_name, "Genre: " %>
    <%= f.text_field :genre_name %>
    <%= f.submit "Here we go!" %>
<% end %>